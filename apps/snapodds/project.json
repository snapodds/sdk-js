{
  "projectType": "application",
  "root": "apps/snapodds",
  "sourceRoot": "apps/snapodds/src",
  "prefix": "snapodds",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/snapodds",
        "index": "apps/snapodds/src/index.html",
        "main": "apps/snapodds/src/main.ts",
        "polyfills": "apps/snapodds/src/polyfills.ts",
        "tsConfig": "apps/snapodds/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/snapodds/src/favicon.ico",
          "apps/snapodds/src/assets",
          {
            "glob": "snapodds-builder.js",
            "input": "dist/apps/snapodds-builder",
            "output": "/assets"
          }
        ],
        "styles": ["apps/snapodds/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/snapodds/src/styles"]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "3kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/snapodds/src/environments/environment.ts",
              "with": "apps/snapodds/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "none"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "snapodds:build:production",
          "ssl": true
        },
        "development": {
          "browserTarget": "snapodds:build:development",
          "ssl": true
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "snapodds:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/snapodds/src/**/*.ts", "apps/snapodds/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/snapodds"],
      "options": {
        "jestConfig": "apps/snapodds/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
