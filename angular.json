{
  "version": 1,
  "projects": {
    "snapodds": {
      "projectType": "application",
      "root": "apps/snapodds",
      "sourceRoot": "apps/snapodds/src",
      "prefix": "snapodds",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/snapodds",
            "index": "apps/snapodds/src/index.html",
            "main": "apps/snapodds/src/main.ts",
            "polyfills": "apps/snapodds/src/polyfills.ts",
            "tsConfig": "apps/snapodds/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/snapodds/src/favicon.ico", "apps/snapodds/src/assets"],
            "styles": ["apps/snapodds/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/snapodds/src/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "3kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/snapodds/src/environments/environment.ts",
                  "with": "apps/snapodds/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "snapodds:build:production",
              "ssl": true
            },
            "development": {
              "browserTarget": "snapodds:build:development",
              "ssl": true
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "snapodds:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/snapodds/src/**/*.ts", "apps/snapodds/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/snapodds"],
          "options": {
            "jestConfig": "apps/snapodds/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "snapodds-api": {
      "root": "apps/snapodds-api",
      "sourceRoot": "apps/snapodds-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/snapodds-api",
            "main": "apps/snapodds-api/src/main.ts",
            "tsConfig": "apps/snapodds-api/tsconfig.app.json",
            "assets": ["apps/snapodds-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/snapodds-api/src/environments/environment.ts",
                  "with": "apps/snapodds-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "snapodds-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/snapodds-api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/snapodds-api"],
          "options": {
            "jestConfig": "apps/snapodds-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
