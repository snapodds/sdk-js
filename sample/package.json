{
  "name": "snapodds-sample",
  "version": "1.0.1",
  "license": "MIT",
  "author": "Snapscreen",
  "main": "src/main.ts",
  "scripts": {
    "copy:assets": "cpx 'src/assets/**' 'dist/assets' && cpx 'src/certs/**' 'dist/certs' && cpx '../dist/elements/snapodds/snapodds-sdk.js' 'dist/assets/js' && cpx '../dist/elements/snapodds/snapodds-sdk.css' 'dist/assets/css'",
    "build": "tsc && npm run copy:assets",
    "start": "npm run build && node -r dotenv/config dist/main.js"
  },
  "dependencies": {
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "typescript": "^4.5.5",
    "cpx": "^1.5.0"
  }
}
