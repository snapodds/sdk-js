{
  "root": "libs/response",
  "sourceRoot": "libs/response/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/response",
        "main": "libs/response/src/index.ts",
        "tsConfig": "libs/response/tsconfig.lib.json",
        "assets": ["libs/response/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/response/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/response"],
      "options": {
        "jestConfig": "libs/response/jest.config.js",
        "passWithNoTests": true
      }
    },
    "docs": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx typedoc --tsconfig libs/response/tsconfig.lib.json --out docs/libs libs/response/src/lib/typings/index.ts"
          }
        ]
      }
    }
  },
  "tags": []
}
